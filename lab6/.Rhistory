#install.packages("skimr")
library("skimr")
msleep
names(msleep)
library("palmerpenguins")
palmerpenguins::penguins_raw
msleep24 <- msleep %>%
mutate(sleep_total_24 = sleep_total / 24) %>%
select(name, vore, sleep_total_24) %>%
filter(!is.na(vore)) %>%
filter(sleep_total_24 < 0.3) %>%
arrange(desc(sleep_total_24))
summary(msleep24)
skim(msleep24)
